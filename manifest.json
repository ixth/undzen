{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 3,
  "name": "Undzen Yandex",
  "version": "1.1",

  "description": "Adds a redirect from yandex.ru to Yandex home page",

  "icons": {
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "https://yandex.ru/",
    "https://dzen.ru/",
    "https://zen.yandex.ru/"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}